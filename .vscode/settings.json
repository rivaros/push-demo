{
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "[xml]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true
    },
    "ecs.enable": false,
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "ios/build": true,
        "ios/Pods": true
    },
    "terminal.integrated.scrollback": 300000
}